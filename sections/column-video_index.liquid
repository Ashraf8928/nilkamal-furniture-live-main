<div class="">

      {%- for block in section.blocks -%}

      <div class="video">
        {% if section.settings.title == blank %} title-wrapper-with-link--no-heading{% endif %}
        <iframe id="main_video_sec"
          src="https://www.youtube.com/embed/{{ block.settings.iframe_vd.id }}?enablejsapi=1&showinfo=0&rel=0&mute=1&autohide=1&controls=1&mute=1&loop=0&autopause=0&modestbranding=1"
          allow="encrypted-media" frameborder="0"></iframe>
      </div>
      {%- endfor -%}
</div>



{% schema %}
{
"name": "Video Section",
"settings": [
{
"type": "text",
"id": "title",
"label": "Heading"
},
{
"type": "image_picker",
"id": "bg_img",
"label": "Background image"
},

{
"type": "image_picker",
"id": "mob_image",
"label": "Mobile_Image"
},
{
"type": "header",
"content": "Padding"
},
{
"type": "range",
"id": "padding_top",
"min": 0,
"max": 100,
"step": 4,
"unit": "px",
"label": "Padding top",
"default": 36
},
{
"type": "range",
"id": "padding_bottom",
"min": 0,
"max": 100,
"step": 4,
"unit": "px",
"label": "Padding bottom",
"default": 36
}
],
"blocks": [
{
"type": "VideoBlog",
"name": "Video Blog",
"settings": [
{
"type": "video_url",
"id": "iframe_vd",
"label": "First Video link",
"accept": ["youtube", "vimeo"],
"info": "Supports Youtube and Vimeo"
}
]
}
],
"presets": [
{
"name": "Video Blog"
}
]
}
{% endschema %}